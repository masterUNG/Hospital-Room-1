<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kaewmod.hospitalroom.DiseasebodyActivity"
    android:background="#f7dcfe">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/DisBody"
            android:id="@+id/textView65"
            android:textColor="#000000"
            android:textSize="@dimen/h5" />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout5">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </TableLayout>
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Disease8"
        android:id="@+id/textView70"
        android:layout_below="@+id/linearLayout5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="105dp"
        android:textSize="@dimen/h2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Disease4"
        android:id="@+id/textView66"
        android:layout_below="@+id/textView70"
        android:layout_alignParentLeft="true"
        android:textSize="@dimen/h2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Disease5"
        android:id="@+id/textView67"
        android:layout_below="@+id/textView66"
        android:layout_alignParentLeft="true"
        android:textSize="@dimen/h2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Disease6"
        android:id="@+id/textView68"
        android:layout_below="@+id/textView67"
        android:layout_alignParentLeft="true"
        android:textSize="@dimen/h2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Disease7"
        android:id="@+id/textView69"
        android:layout_below="@+id/textView68"
        android:layout_alignParentLeft="true"
        android:textSize="@dimen/h2" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </LinearLayout>

    <ImageView
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:id="@+id/imageView33"
        android:background="@drawable/bodyy"
        android:layout_gravity="center"
        android:layout_below="@+id/textView69"
        android:layout_toRightOf="@+id/textView69"
        android:layout_toEndOf="@+id/textView69"
        android:layout_marginTop="47dp" />
</RelativeLayout>
