<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kaewmod.hospitalroom.DiseaseheaderActivity"
    android:background="@drawable/background">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:id="@+id/imageView51"
                android:background="@drawable/homme1"
                android:onClick="clickMenuNurseBody"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="19dp"
                android:layout_marginEnd="19dp" />

            <TextView
                android:layout_width="284dp"
                android:layout_height="wrap_content"
                android:text="@string/DisBody"
                android:id="@+id/textView61"
                android:textSize="@dimen/h5"
                android:textColor="@color/colorPrimaryDark"
                android:layout_gravity="center_vertical"
                android:textStyle="normal|bold" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout4">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </TableLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimary"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/linearLayout">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Disease6"
        android:id="@+id/textView64"
        android:textSize="@dimen/h5"
        android:background="#6400ddff"
        android:textColor="#000000"
        android:layout_below="@+id/textView63"
        android:layout_alignParentLeft="true"
        android:onClick="clickDisbody3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Disease4"
        android:id="@+id/textView62"
        android:textSize="@dimen/h5"
        android:background="#6400ddff"
        android:textColor="#000000"
        android:onClick="clickDisbody1"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="@string/Disease8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView20"
        android:layout_below="@+id/textView19"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/h5"
        android:textColor="@android:color/background_dark"
        android:background="#6400ddff"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="clickDisbody5" />

    <TextView
        android:text="@string/Disease7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:layout_below="@+id/textView64"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/h5"
        android:textColor="@android:color/background_dark"
        android:background="#3200ddff"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="clickDisbody4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Disease5"
        android:id="@+id/textView63"
        android:textSize="@dimen/h5"
        android:background="#3200ddff"
        android:textColor="#000000"
        android:onClick="clickDisbody2"
        android:layout_below="@+id/textView62"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="180dp"
        android:id="@+id/imageView32"
        android:background="@drawable/hbll2"
        android:layout_marginTop="31dp"
        android:layout_below="@+id/linearLayout"
        android:layout_centerHorizontal="true"
        android:layout_height="120dp" />
</RelativeLayout>