<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kaewmod.hospitalroom.MenuActivity"
    android:background="@drawable/bggg">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout6">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </LinearLayout>
    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu1"
        android:id="@+id/button29"
        android:background="#4f51cc"
        android:layout_above="@+id/imageView31"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/h"
        android:textColor="#ffffff"
        android:nestedScrollingEnabled="false"
        android:onClick="clickMenuHome" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu2"
        android:id="@+id/button30"
        android:layout_alignTop="@+id/button29"
        android:layout_centerHorizontal="true"
        android:background="#4f51cc"
        android:textSize="@dimen/h"
        android:textColor="#ffffff"
        android:onClick="clickMenuProfile" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu3"
        android:id="@+id/button31"
        android:background="#4f51cc"
        android:layout_above="@+id/imageView31"
        android:layout_alignLeft="@+id/imageView31"
        android:layout_alignStart="@+id/imageView31"
        android:textSize="@dimen/h"
        android:textColor="#ffffff"
        android:onClick="clickMenuBody" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout8">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:layout_width="125dp"
        android:layout_height="100dp"
        android:id="@+id/imageView29"
        android:background="@drawable/aoao"
        android:layout_marginBottom="101dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="105dp"
        android:id="@+id/imageView30"
        android:background="@drawable/mdchai"
        android:layout_alignTop="@+id/imageView29"
        android:layout_toRightOf="@+id/imageView29"
        android:layout_toEndOf="@+id/imageView29" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="100dp"
        android:id="@+id/imageView31"
        android:background="@drawable/yoga"
        android:layout_above="@+id/button34"
        android:layout_toRightOf="@+id/imageView30"
        android:layout_toEndOf="@+id/imageView30" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </LinearLayout>
    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu4"
        android:id="@+id/button32"
        android:background="#4f51cc"
        android:layout_below="@+id/imageView30"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="@dimen/h"
        android:textColor="#ffffff"
        android:layout_alignRight="@+id/button29"
        android:layout_alignEnd="@+id/button29"
        android:onClick="clickMenuSave" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu6"
        android:id="@+id/button34"
        android:background="#4f51cc"
        android:layout_below="@+id/imageView30"
        android:layout_alignLeft="@+id/imageView31"
        android:layout_alignStart="@+id/imageView31"
        android:textSize="@dimen/h"
        android:textColor="#ffffff"
        android:onClick="clickMenuYoga" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu5"
        android:id="@+id/button33"
        android:background="#4f51cc"
        android:layout_alignTop="@+id/button34"
        android:textSize="@dimen/h"
        android:textColor="#ffffff"
        android:layout_alignRight="@+id/button30"
        android:layout_alignEnd="@+id/button30"
        android:layout_alignLeft="@+id/button30"
        android:layout_alignStart="@+id/button30"
        android:onClick="clickMenuMadchin" />

    <ImageView
        android:layout_width="125dp"
        android:layout_height="100dp"
        android:id="@+id/imageView28"
        android:background="@drawable/three"
        android:layout_alignBottom="@+id/imageView27"
        android:layout_alignRight="@+id/imageView31"
        android:layout_alignEnd="@+id/imageView31" />

    <ImageView
        android:layout_width="115dp"
        android:layout_height="100dp"
        android:id="@+id/imageView25"
        android:background="@drawable/user"
        android:layout_alignBottom="@+id/imageView27"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="115dp"
        android:layout_height="95dp"
        android:id="@+id/imageView27"
        android:background="@drawable/home"
        android:layout_above="@+id/button29"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Hospital"
        android:id="@+id/textView74"
        android:textColor="#000000"
        android:textSize="@dimen/h4"
        android:layout_gravity="center_vertical"
        android:textStyle="bold"
        android:layout_above="@+id/imageView28"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
