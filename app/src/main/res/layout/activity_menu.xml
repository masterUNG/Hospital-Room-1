<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kaewmod.hospitalroom.MenuActivity"
    android:background="@drawable/bggg">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Hospital"
        android:id="@+id/textView74"
        android:textColor="#000000"
        android:textSize="@dimen/h4"
        android:layout_gravity="center_vertical"
        android:textStyle="bold"
        android:background="#c3ffffff"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="145dp"
        android:id="@+id/imageView25"
        android:background="@drawable/three"
        android:layout_below="@+id/textView74"
        android:layout_alignLeft="@+id/textView74"
        android:layout_alignStart="@+id/textView74"
        android:layout_marginTop="64dp" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/imageView27"
        android:background="@drawable/aoao"
        android:layout_alignBottom="@+id/imageView25"
        android:layout_toRightOf="@+id/imageView25"
        android:layout_toEndOf="@+id/imageView25" />

</RelativeLayout>
