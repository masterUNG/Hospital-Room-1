<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kaewmod.hospitalroom.MenuActivity"
    android:background="@drawable/background">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#96ffffff"
        android:id="@+id/linearLayout2">

    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Hospital"
        android:id="@+id/textView74"
        android:textColor="#5e53fe"
        android:textSize="@dimen/h4"
        android:layout_gravity="center_vertical"
        android:textStyle="bold"
        android:background="#c3ffffff"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/imageView27"
        android:background="@drawable/halety"
        android:layout_alignBottom="@+id/imageView28"
        android:layout_alignRight="@+id/textView74"
        android:layout_alignEnd="@+id/textView74" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu4"
        android:id="@+id/button30"
        android:textSize="@dimen/h"
        android:background="@drawable/abc_list_longpressed_holo"
        android:textColor="#ffffff"
        android:onClick="clickMenuSave"
        android:layout_below="@+id/imageView27"
        android:layout_alignRight="@+id/textView74"
        android:layout_alignEnd="@+id/textView74"
        tools:textStyle="bold" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </LinearLayout>

    <ImageView
        android:layout_width="150dp"
        android:layout_height="145dp"
        android:id="@+id/imageView28"
        android:background="@drawable/medi"
        android:layout_below="@+id/button31"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="145dp"
        android:id="@+id/imageView29"
        android:background="@drawable/yoga"
        android:layout_below="@+id/textView74"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="73dp" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </LinearLayout>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu6"
        android:id="@+id/button31"
        android:textSize="@dimen/h"
        android:layout_below="@+id/imageView29"
        android:background="@drawable/abc_list_longpressed_holo"
        android:textColor="#ffffff"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="clickMenuYoga"
        android:layout_alignRight="@+id/imageView29"
        android:layout_alignEnd="@+id/imageView29"
        tools:textStyle="bold"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu5"
        android:id="@+id/button32"
        android:textSize="@dimen/h"
        android:background="@drawable/abc_list_longpressed_holo"
        android:textColor="#ffffff"
        android:onClick="clickMenuMadchin"
        android:layout_alignTop="@+id/button30"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        tools:textStyle="bold" />

    <ImageView
        android:layout_width="155dp"
        android:layout_height="145dp"
        android:id="@+id/imageView30"
        android:background="@drawable/three"
        android:layout_above="@+id/button33"
        android:layout_alignLeft="@+id/button33"
        android:layout_alignStart="@+id/button33" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Menu3"
        android:id="@+id/button33"
        android:textSize="@dimen/h"
        android:layout_above="@+id/imageView28"
        android:layout_alignLeft="@+id/imageView27"
        android:layout_alignStart="@+id/imageView27"
        android:background="@drawable/abc_list_longpressed_holo"
        android:textColor="#ffffff"
        android:onClick="clickMenuBody"
        android:layout_alignRight="@+id/imageView27"
        android:layout_alignEnd="@+id/imageView27"
        tools:textStyle="bold" />

</RelativeLayout>
